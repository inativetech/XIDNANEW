@model XIDNA.ViewModels.VMViewPopup


@{
    Layout = "~/Views/Shared/_PopupLayout.cshtml";
}

<section class="content-header hedr">
    <h1> Outbound User </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> User</a></li>
        <li class="active">Home</li>
    </ol>
</section>
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-body">
                    @using (Ajax.BeginForm("", "Popup", new AjaxOptions { HttpMethod = "POST", UpdateTargetId = "Field", OnSuccess = "OnComplete()" }, new { @id = "TabsForm", @class = "form-horizontal" }))
                    {
                        @Html.ValidationSummary(true);
                        <div class="form-group">
                        <div class="col-md-3">
                            <label for="inputEmail" class="gn">Outbound User<span class="danger"></span></label>
                            @Html.DropDownListFor(m => m.LeadID, new SelectList(Model.Users, "value", "text"), "--Select--", new { @class = "form-control", @id = "User" })
                        </div>
                            <div class="col-sm-7 reporterrormsg">
                                @Html.ValidationMessageFor(m => m.LeadID, null, new { @class = "red" })
                            </div>
                    </div>
                        <div class="form-group">
                            <div class="col-sm-10">
                                <button type="submit" class="btn btn-theme" id="SaveTab">Save</button>
                                <button type="button" class="btn btn-theme decline" id="CancelTab">Cancel</button>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>

</section>