            $('#createif_login').on('show.bs.modal', function (e) {
                setTimeout(function () {
                    $('.modal-backdrop').addClass('createif_login-backdrop');
                });
            });
            $(window).on('load', function () {
                $('#createif_login').modal('show');
            });
            $(function () {
                $(window).scroll(function () {
                    var scroll = $(window).scrollTop();
                    //var height = $('.page-wrapper').height() - 40;
                    var height = $('.page-wrapper').height() + $('.section-wrapper').height() - 40;
                    if (scroll >= height) {
                        $('.categoriessec').addClass('fixMenu');
                    } else {
                        $('.categoriessec').removeClass('fixMenu');
                    };
                });
            });
        </script>
        <script>
            ///////////////Scroll to Top///////////////
            $(window).scroll(function () {
                if ($(this).scrollTop() >= 50) { // If page is scrolled more than 50px
                    $('#section-head').addClass('headerFix');
                    $('#return-to-top').fadeIn(200); // Fade in the arrow
                } else {
                    $('#section-head').removeClass('headerFix');
                    $('#return-to-top').fadeOut(200); // Else fade out the arrow
                }
            });
            returnToTop = function () { // When arrow is clicked
                $('body,html').animate({ scrollTop: 0 }, 500); // Scroll to top of body
                return false;
            };
            //////////////////////////////////////
            //MegaMenu//
            $(document).ready(function () {
                "use strict";

                $('.menu > ul > li:has( > ul)').addClass('menu-dropdown-icon');
                $('.menu > ul > li > ul:not(:has(ul))').addClass('normal-sub');
                $(".menu > ul").before("<a href=\"\" class=\"menu-mobile\"><span class=\"one\"></span><span class=\"two\"></span><span class=\"three\"></span></a>");

                $(".menu > ul > li").hover(function (e) {
                    if ($(window).width() > 943) {
                        $(this).children("ul").stop(true, false).fadeToggle(150);
                        e.preventDefault();
                    }
                });

                $(".menu > ul > li").click(function () {
                    if ($(window).width() <= 943) {
                        $(this).children("ul").fadeToggle(150);
                        $(this).toggleClass("expand").siblings().removeClass('expand');
                        $(this).siblings().children("ul").hide();
                    }
                });

                $(".menu-mobile").click(function (e) {
                    $(".menu > ul").toggleClass('show-on-mobile');
                    e.preventDefault();
                });

                $(".menu-mobile").click(function () {
                    $(".menu-mobile").toggleClass('crossed');
                });
                // Video Slide
                document.getElementById('vid').paly();
            });

            $(window).on('load scroll', function () {
                var scrolled = $(this).scrollTop();
                $('#home-slide #vid').css('transform', 'translate3d(0, ' + -(scrolled * 0.25) + 'px, 0)'); // parallax (25% scroll rate)
            });
            $(window).resize(function () {
                $('#section-banner').css('height', ($(window).height() - $('#section-head').height()));
            });
            $(window).trigger('resize');
            //Smooth scroll btn
            $(".banner-down-btn").on('click', function () {
                $(".section-wrapper").animate({
                    scrollTop: $(".section-body").offset().top
                }, 'slow');
            });
        </script>
