        <div class="login-box">
            <div class="login-box-logo">
                <img src="~/Content/images/@ViewBag.Logo">
            </div>
            <div class="login-box-body">
                <h4 class="login-box-msg-head styl-p font-white uppercase m-t-none">@ViewBag.AppName<span class="style-p small font-white pull-right login-box-msg">Log-in</span></h4>
                @using (Html.BeginForm("Login", "Account", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <div class="form-group ">
                        @Html.TextBoxFor(m => m.UserName, new { @class = "ione form-control has-feedback", @placeholder = "User Name" })
                        <span class="fas fa-user form-control-feedback"></span>
                    </div>
                    <div class="form-group">
                        @Html.PasswordFor(m => m.Password, new { @class = "itwo form-control has-feedback", @placeholder = "Password", @onCopy = "return false", @onDrag = "return false", @onDrop = "return false", @onPaste = "return false", @autocomplete = "off" })
                        <span class="fas fa-lock form-control-feedback"></span>
                    </div>
                    <div class="form-group text-right m-t-15">
                        @Ajax.ActionLink("Forgot Password?",
                       "ForgotPassword", "Account",
                     new AjaxOptions
                     {
                         UpdateTargetId = "ForgotPwdDiv",
                         InsertionMode = InsertionMode.Replace,
                         HttpMethod = "GET",
                         LoadingElementId = "ajax-loader",
                     }
                     )
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-submit">Login</button>
                    </div>

                }
                <p class="small no-margin">Copyright © 2021</p>
            </div>
        </div>
    </body>
